{
    "manifest_version": 3,
    "name": "TeraBox Video Extension",
    "version": "1.0",
    "description": "A Chrome extension to download or watch TeraBox videos.",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  